  "jsnext:main": "plain-overlay.esm.js",
  "files": [
    "plain-overlay?(-limit)?(-sync)?(-debug).@(min.js|esm.js)",
    "bower.json"
  ],
  "devDependencies": {
    "anim-event": "^1.0.15",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
