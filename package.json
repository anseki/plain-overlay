    "cssprefix": "^2.0.15",
    "htmlclean": "^3.0.8",
    "htmlclean-loader": "^1.0.12",
    "jasmine-core": "^3.1.0",
    "log4js": "^2.5.3",
    "m-class-list": "^1.1.8",
    "node-sass": "^4.8.3",
    "node-static-alias": "^1.0.0",
    "post-compile-webpack-plugin": "^0.1.2",
    "pre-proc": "^1.0.2",
    "pre-proc-loader": "^3.0.3",
    "sass-loader": "^6.0.7",
    "skeleton-loader": "^2.0.0",
