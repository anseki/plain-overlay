    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --verbose",
    "dev": "webpack --verbose",
    "build-limit": "cross-env EDITION=limit NODE_ENV=production webpack --verbose",
    "dev-limit": "cross-env EDITION=limit webpack --verbose",
